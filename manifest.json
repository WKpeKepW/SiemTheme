{
   "manifest_version": 3,
   "name": "SiemTheme",
   "version": "0.2.5",
   "description": "Изменение темы Siem",
   "content_scripts": [
       {
           "matches": ["http://*/*","https://*/*"],
           "js": [ "lib//jquery-3.7.1.min.js","lib//Core.js","lib//NewCore.js","Main.js",
        "Themes//InvertTheme.js","Themes//InvertThemeBeta.js"]
       }
   ],
   "icons": {
    "16":"icons/16.png",
    "32":"icons/32.png",
    "48":"icons/48.png",
    "128":"icons/128.png"
},
"action": {
    "default_popup":"popup//popup.html"
},
"content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self'"
},
"permissions":["storage","tabs"]
}
