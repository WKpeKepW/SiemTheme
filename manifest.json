{
   "manifest_version": 3,
   "name": "SiemTheme",
   "version": "0.1.0",
   "description": "Изменение темы Siem",
   "content_scripts": [
       {
           "matches": ["<all_urls>"],
           "js": [ "lib//jquery-3.7.1.min.js","lib//Core.js","Main.js",
        "Themes//InvertTheme.js" ]
       }
   ],
   "icons": {
    "16":"icons/16.png",
    "32":"icons/32.png",
    "48":"icons/48.png",
    "128":"icons/128.png"
},
"action": {
    "default_popup":"popup//popup.html"
}
}
